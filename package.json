{
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "concurrently -r pnpm:build:*",
    "build:js": "node build.mjs",
    "build:css": "postcss Resources/Private/Assets/*.pcss --no-map --ext css --dir Resources/Public/Styles/",
    "watch": "concurrently -r pnpm:watch:*",
    "watch:js": "node build.mjs --watch",
    "watch:css": "postcss Resources/Private/Assets/*.pcss --no-map --ext css --dir Resources/Public/Styles/ -w",
    "pipeline": "pnpm install --silent --non-interactive;pnpm build"
  },
  "devDependencies": {
    "@alpinejs/collapse": "^3.14.1",
    "@alpinejs/focus": "^3.14.1",
    "@neos-project/neos-ui-extensibility": "~8.3.10",
    "alpinejs": "^3.14.1",
    "autoprefixer": "^10.4.20",
    "clsx": "^2.1.1",
    "concurrently": "^9.0.1",
    "cssnano": "^7.0.6",
    "esbuild": "^0.24.0",
    "esbuild-plugin-lightningcss-modules": "^0.1.2",
    "flatpickr": "^4.6.13",
    "postcss": "^8.4.47",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.13",
    "tippy.js": "^6.3.7"
  }
}
