prototype(Garagist.Mautic:Component.Form) < prototype(Neos.Fusion:Component) {
    id = ${q(node).property('formId')}
    url = ${Configuration.Setting('Garagist.Mautic.publicUrl')}

    @if.hasUrlAndId = ${this.url && this.id}

    renderer = afx`
        <script type="text/javascript" src={props.url + "/form/generate.js?id=" + props.id}></script>
    `
}
