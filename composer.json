{
    "description": "Neos adapter for the Mautic API",
    "type": "neos-plugin",
    "name": "garagist/mautic",
    "require": {
        "ext-intl": "*",
        "ext-json": "*",
        "neos/neos-ui": "^7.1",
        "neos/event-sourcing": "^2.0",
        "neos/neos": "^7.1",
        "neos/fusion-form": "^2.0",
        "mautic/api-library": "^3.0.0",
        "carbon/eel": "^1.13.3 || ^2.0"
    },
    "autoload": {
        "psr-4": {
            "Garagist\\Mautic\\": "Classes/"
        }
    },
    "extra": {
        "neos": {
            "package-key": "Garagist.Mautic"
        }
    },
    "archive": {
        "exclude": [
            ".github",
            ".editorconfig",
            ".eslintignore",
            ".eslintrc",
            ".gitattributes",
            ".gitignore",
            ".jshintrc",
            ".nvmrc",
            ".postcssrc.yml",
            ".prettierignore",
            ".prettierrc",
            ".stylelintrc",
            ".yarnclean",
            "CODE_OF_CONDUCT.md",
            "package.json",
            "tailwind.config.js",
            "yarn.lock"
        ]
    },
    "authors": [
        {
            "name": "David Spiola",
            "role": "Developer"
        },
        {
            "name": "Jon Uhlmann",
            "email": "hello@uhlmann.pro",
            "homepage": "https://www.uhlmann.pro",
            "role": "Developer"
        }
    ]
}
