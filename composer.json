{
    "description": "Neos adapter for the Mautic API",
    "type": "neos-plugin",
    "name": "garagist/mautic",
    "require": {
        "neos/neos-ui": "^7.1",
        "neos/event-sourcing": "^2.0",
        "neos/neos": "^7.1",
        "mautic/api-library": "^2.0.0"
    },
    "autoload": {
        "psr-4": {
            "Garagist\\Mautic\\": "Classes/"
        }
    },
    "extra": {
        "neos": {
            "package-key": "Garagist.Mautic"
        }
    },
    "archive": {
        "exclude": [
            ".github",
            ".editorconfig",
            ".eslintignore",
            ".eslintrc",
            ".gitattributes",
            ".gitignore",
            ".jshintrc",
            ".nvmrc",
            ".postcssrc.yml",
            ".prettierignore",
            ".prettierrc",
            ".stylelintrc",
            ".yarnclean",
            "CODE_OF_CONDUCT.md",
            "package.json",
            "tailwind.config.js",
            "yarn.lock"
        ]
    }
}
